  "version": "2.0.0",
  "tasks": [
    {
      "type": "flutter",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "watch",
        "--delete-conflicting-outputs"
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "label": "flutter: flutter pub run build_runner watch --delete-conflicting-outputs"
    },
    {
      "type": "flutter",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "build"
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "label": "flutter: flutter pub run build_runner build"
    },
    {
      "type": "flutter",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "serve"
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "label": "flutter: flutter pub run build_runner serve"
    },
    {
      "type": "flutter",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "watch"
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "label": "flutter: flutter pub run build_runner watch"
    }
  ]
}
